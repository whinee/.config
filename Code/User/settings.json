        "bash": {
            "path": "bash",
            "args": [
                "--init-file",
                "/home/whine/.config/bash/rc"
            ]
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-c",
                "ZDOTDIR=$XDG_CONFIG_HOME/zsh/ zsh"
