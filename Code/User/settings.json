        "dev/constants/dict/**/*",
        "**/req.txt",
        "**/requirements.txt",
        "**/*.mp"
    ],
    "files.exclude": {
        // false
        "**/.git*": false,
        // true
        "**/__pycache__": true,
        "**/*.egg-info/": true,
        ".history": true,
        "node_modules": true,
        "venv": true,
        "pyenv": true
    },
    "search.exclude": {
        "**/dist": true,
        "**/node_modules": true,
        "**/pyenv": true,
        "**/tmp": true,
        "**/venv": true
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.gzip": "default",
        "*.db": "default"
    },
    "diffEditor.codeLens": true,
    "editor.suggestSelection": "first",
    "telemetry.telemetryLevel": "off",
    "syntax.highlightTerms": [
        "type",
        "scope",
        "function",
        "variable",
        "number",
        "string",
        "comment",
        "constant",
        "directive",
        "control",
        "operator",
        "modifier",
        "punctuation"
    ],
